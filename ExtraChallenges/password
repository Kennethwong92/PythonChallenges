import random
end_character = random.randrange(100,100000)
password = input("Would you like a random password generated? Y/N")
if password == ("Y"):
    word = input("Whats your favorite color?")
    l = list(word)
    random.shuffle(l)
    comp=""
    for i in l:
        comp+=i
    result = comp + str(end_character)
    print("Your password is:", result + "$%#*&")
