print("ex2")
import turtle
cellphone = turtle.Turtle()
# 3 Attributes
cellphone.turtle.colour("lightgreen")
cellphone.speed(100)
cellphone.turtle.size(100)
# 3 Methods
cellphone.forward(100)
cellphone.backward(100)
cellphone.stamp()

print("ex3")
