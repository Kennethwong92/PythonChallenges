print("ex7")
import turtle

wn = turtle.Screen()
for angle in [160, -43, 270, -97, -43, 200, -940, 17, -86]:

    turtle.left(angle)
    turtle.forward(100)
print('the pirate has a heading of', turtle.heading())
turtle.done()

