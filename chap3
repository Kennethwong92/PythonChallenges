print("ex11")
import turtle
wn = turtle.Screen()
wn.bgcolor("lightgreen")
turtle.shape("turtle")
turtle.color("blue")
turtle.pensize(5)
turtle.stamp()
for loop in range (0, 12):
    turtle.penup()
    turtle.forward(80)
    turtle.pendown()
    turtle.forward(10)
    turtle.penup()
    turtle.forward(20)
    turtle.pendown()
    turtle.stamp()
    turtle.penup()
    turtle.backward(110)
    turtle.pendown()
    turtle.right(30)
turtle.done()
