    month = "One of the months of the year is " + f
    print(month)

print("ex4")
import turtle
wn = turtle.Screen()
tess = turtle.Turtle()
tess.left(3645)

print(3645 // 360)
print(3645 % 360)
print("turns around until 10 times and then faces north east")

print("ex5")
for q in [12, 10, 32, 3, 66, 17, 42, 99, 20]:
    total = 0
    x = total + q + q
    print(q, q ** 2, x)

